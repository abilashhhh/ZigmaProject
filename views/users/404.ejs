<%- include('../partials/userHeader.ejs') %>


        <main class="main page-404">
            <div class="container">
                <div class="row align-items-center height-100vh text-center">
                    <div class="col-lg-8 m-auto">
                        <p class="mb-50"><img src="assets/imgs/theme/404.png" alt="" class="hover-up"></p>
                        <h2 class="mb-30">Page Not Found</h2>
                        <p class="font-lg text-grey-700 mb-30">
                            The link you clicked may be broken or the page may have been removed.<br> visit the <a
                                href="/"> <span> Homepage</span></a> or <a href="/contactPage"><span>Contact
                                    us</span></a> about the problem
                        </p>

                        <h3 style="color: red;">
                            <% if(message && typeof message !=='undefined' ){%>
                                <%= message %>
                            <% } %>
                        </h3>
                        <br><br>
                        <p class="mb-50"> <img style="width: 80px; height:70px;" src="/logo.jpg" alt="" class="hover-up"></p>

                    </div>
                </div>
            </div>
        </main>


        <%- include('../partials/userFooter.ejs') %>