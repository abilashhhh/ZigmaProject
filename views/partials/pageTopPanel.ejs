<header class="header-area header-style-5">
    <div class="header-top header-top-ptb-1 d-none d-lg-block">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-xl-3 col-lg-4">
                    <div class="header-info">
                        <ul>
                            <li><i class="fi-rs-smartphone"></i> <a href="/contactPage">(+01) - 2345 - 6789</a></li>
                            <li><i class="fi-rs-marker"></i><a  href="/contactPage">Our location</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-4">
                    <div class="text-center">
                        <div id="news-flash" class="d-inline-block">
                            <ul>
                                <% if (userData && typeof userData !== 'undefined') { %>
                                    <li>Get great smart watches up to 30% off <a href="/shop?categoryName=All Brands">View details</a></li>
                                    <li>Super Value Deals - Save more with coupons</li>
                                    <li>Trendy Apple smart watches, save up to 35% off today <a href="/shop?categoryName=Apple">Shop now</a></li>
                                <% } else { %>
                                   
                                    <li>Get great smart watches up to 30% off <a href="/login">View details</a></li>
                                    <li>Super Value Deals - Save more with coupons</li>
                                    <li>Trendy Apple smart watches, save up to 35% off today <a href="/login">Shop now</a></li>
                                <% } %>
                                
                               
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-xl-3 col-lg-4">
                    <div class="header-info header-info-right">
                        <ul>
                            <li>
                                <a class="language-dropdown-active" href=""> <i class="fi-rs-world"></i> English  </a>
                               
                            </li>
                            <% if (userData && typeof userData !=='undefined' ) { %>
                                <li><a href="/accountPage"><i class="fi-rs-user"></i>
                                        <%= userData.username %>
                                    </a></li>
                                <% } else { %>
                                    <li><i class="fi-rs-user"></i><a href="/login">Log In</a></li>
                                    <li><i class="fi-rs-user"></i><a href="/register">Sign Up</a></li>
                                    <% } %>                              
                          </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="header-bottom sticky-bar sticky-white-bg">
        <div class="container">
            <div class="header-wrap header-space-between position-relative">
                <div class="logo logo-width-1">
                    <a href="/"><img style="margin: 20px;" src="assets/imgs/theme/logo.jpg" alt="logo"></a>
                </div>
                <div class="main-menu main-menu-grow main-menu-padding-1 main-menu-lh-1 main-menu-mrg-1 hm3-menu-padding d-none d-lg-block hover-boder">
                    <nav>
                        <ul>

                            <% if (userData && typeof userData !=='undefined' ) { %>
                                <li><a class="active" href="/">Home </a></li>
                                <li><a href="/shop?categoryName=All Brands">All Products</a></li>
                                <li><a href="/brands">All Brands</a></li>
                                <li><a href="/aboutPage">About</a></li>
                                <li><a href="/blogCategoryFullwidth">Blog</a></li>
                                <li><a href="/blogPostFullwidth">Latest Buzz</a></li>
                                <li><a href="/contactpage">Contact</a></li>
                                <li><a href="/accountpage">My Account</a></li>
                                <% } else { %>
                                 
                                    
                                    <li><a class="active" href="/">Home </a></li>
                                    <li><a href="/login">All Products</a></li><!-- shop page link -->
                                    <li><a href="/brands">All Brands</a></li>
                                    <li><a href="/aboutPage">About</a></li>
                                    <li><a href="/blogCategoryFullwidth">Blog</a></li>
                                    <li><a href="/blogPostFullwidth">Latest Buzz</a></li>
                                    <li><a href="/contactpage">Contact</a></li>
                                    <% } %>   
                           
                             
                     
                        </ul>
                    </nav>
                </div>
                <div class="header-action-right">
                     
                    

                        <% if (userData && typeof userData !=='undefined' ) { %>
                          
                            <div class="header-action-2">
                                <div class="header-action-icon-2" style="margin: 5px;">
                                    <a href="/wishlist">
                                        <img style="width: 25px;" class="svgInject" alt="Zigma Smartwatches"
                                            src="assets/imgs/theme/icons/icon-heart.svg">
                                        <p style="margin-left: -10px ;">Wishlist</p>
                                    </a>
                                </div>
                                <div class="header-action-icon-2" style="margin: 5px;">
                                    <a class="mini-cart-icon" href="/cart">
                                        <img style="width: 25px;" alt="Zigma Smartwatches"
                                            src="assets/imgs/theme/icons/icon-cart.svg">
                                        <p>Cart</p>
                                    </a>
                                </div>
                                <div class="header-action-icon-2 d-block d-lg-none" >
                                    <div class="burger-icon burger-icon-white">
                                        <span class="burger-icon-top"></span>
                                        <span class="burger-icon-mid"></span>
                                        <span class="burger-icon-bottom"></span>
                                    </div>
                                </div>
                            </div>
                            <% } else { %>
                                
                                <div class="header-action-2" style="margin: 5px;">
                                    <div class="header-action-icon-2" style="margin: 5px;">
                                        <a href="/login">
                                            <img style="width: 25px;" class="svgInject" alt="Zigma Smartwatches"
                                                src="assets/imgs/theme/icons/icon-heart.svg">
                                            <p style="margin-left: -10px ;">Wishlist</p>
                                        </a>
                                    </div>
                            

                                    <div class="header-action-icon-2" style="margin: 5px;">
                                        <a class="mini-cart-icon" href="/login">
                                            <img style="width: 25px;" alt="Zigma Smartwatches"
                                                src="assets/imgs/theme/icons/icon-cart.svg">
                                            <p>Cart</p>
                                        </a>
                                    </div>
                                    <div class="header-action-icon-2 d-block d-lg-none">
                                        <div class="burger-icon burger-icon-white">
                                            <span class="burger-icon-top"></span>
                                            <span class="burger-icon-mid"></span>
                                            <span class="burger-icon-bottom"></span>
                                        </div>
                                    </div>
                                </div>
                                <% } %>    


                    
                </div>
                </div>
            </div>
        </div>
    </div>
</header>
